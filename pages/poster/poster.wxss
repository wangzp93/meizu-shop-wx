/* pages/demo/demo.wxss */
/* html */
.outer {
  margin: 20rpx auto 0;
  position: relative;
  box-shadow: 0 0 20rpx #CCC;
}
.outer .wrap {
  position: absolute;
  pointer-events: none;
  width: 100%;
  height: 100%;
}
.outer .text {
  position: absolute;
  pointer-events: none;
}
/* canvas */
.cvs {
  margin: 20rpx auto 0;
  box-shadow: 0 0 20rpx #CCC;
}
/* image */
.image {
  margin: 20rpx auto 0;
  box-shadow: 0 0 20rpx #CCC;
  display: block;
}